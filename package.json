{
    "name": "pluralsight-buildingwith-react-redux",
    "version": "1.0.0",
    "description": "Project based of the Pluralsight course 'Building Applications with React and Redux in ES6' by Cory House",
    "scripts": {
        "prestart": "babel-node tools/startMessage.js",
        "start": "npm-run-all --parallel open:src lint:watch test:watch",
        "open:src": "babel-node tools/srcServer.js",
        "lint": "node_modules/.bin/esw webpack.config.* src tools",
        "lint:watch": "npm run lint -- --watch",
        "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
        "test:watch": "npm run test -- --watch",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "node_modules/.bin/rimraf ./dist",
        "build:html": "babel-node tools/buildHTML.js",
        "prebuild": "npm-run-all clean-dist test lint build:html",
        "build": "babel-node tools/build.js",
        "postbuild": "babel-node tools/distServer.js"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^3.3.7",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.6",
        "react-redux-toastr": "^7.2.1",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "redux": "^3.7.2",
        "redux-form": "^7.2.3",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.2.0",
        "reselect": "^3.0.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^6.4.1",
        "babel-plugin-react-display-name": "2.0.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-register": "^6.26.0",
        "cheerio": "^1.0.0-rc.2",
        "colors": "^1.3.2",
        "compression": "^1.7.3",
        "cross-env": "1.0.7",
        "css-loader": "^0.28.11",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.6.0",
        "eslint": "^4.19.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-react": "^7.11.1",
        "eslint-watch": "^3.1.5",
        "eventsource-polyfill": "^0.9.6",
        "expect": "^22.4.3",
        "express": "^4.16.3",
        "extract-text-webpack-plugin": "1.0.1",
        "file-loader": "0.8.5",
        "jsdom": "8.5.0",
        "mocha": "^5.2.0",
        "nock": "^9.6.1",
        "node-sass": "^4.9.3",
        "npm-run-all": "1.8.0",
        "open": "0.0.5",
        "react-test-renderer": "^16.5.2",
        "react-test-utils": "0.0.1",
        "react-transform-hmr": "^1.0.4",
        "redux-immutable-state-invariant": "^2.1.0",
        "redux-mock-store": "^1.5.3",
        "rimraf": "^2.6.2",
        "sass-loader": "git+https://github.com/webpack-contrib/sass-loader.git#archive/webpack-1",
        "style-loader": "^0.20.3",
        "url-loader": "^0.6.2",
        "webpack": "1.13.0",
